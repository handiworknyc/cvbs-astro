{
  "name": "@example/minimal",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "sync:flex:dev": "node --env-file=.env.development src/scripts/sync-flex-rest.js",
    "sync:flex:prod": "node --env-file=.env.production src/scripts/sync-flex-rest.js",

    "predev": "npm run sync:flex:dev",
    "dev": "astro dev",

    "prebuild": "npm run sync:flex:prod",
    "build": "astro build",

    "preview": "astro preview",
    "astro": "astro",

    "dev:localtls": "NODE_EXTRA_CA_CERTS=./clbr.local.cert npm run dev",
    "dev:mkcert": "NODE_EXTRA_CA_CERTS=\"$HOME/Library/Application Support/mkcert/rootCA.pem\" npm run dev",
    "dev:noverify": "NODE_TLS_REJECT_UNAUTHORIZED=0 npm run dev"
  },
  "dependencies": {
    "@astrojs/netlify": "^6.5.11",
    "@astrojs/react": "^4.4.0",
    "astro": "^5.14.1",
    "geist": "^1.5.1",
    "gsap": "^3.13.0",
    "locomotive-scroll": "^5.0.0-beta.21",
    "motion": "^12.23.22",
    "motion-plus": "https://api.motion.dev/registry?package=motion-plus&version=1.5.4&token=4d9d2a16fce72996f44a3548c4893c2e84e5860e868d75e26663f9f9b9baf4df",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@astrojs/tailwind": "^6.0.2",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.4.5",
    "postcss": "^8.5.6",
    "sass": "^1.93.2",
    "tailwindcss": "^3.4.17"
  },
  "engines": {
    "node": ">=20"
  }
}
